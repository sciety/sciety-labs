[MAIN]
extension-pkg-whitelist=
    lxml

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    too-many-positional-arguments

ignored-classes=
    googleapiclient.discovery.Resource

[SIMILARITIES]
min-similarity-lines=10
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
